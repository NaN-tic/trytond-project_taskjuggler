<form string="TaskJuggler">
    <label name="name"/>
    <field name="name"/>
    <label name="start_date"/>
    <field name="start_date"/>
    <label name="duration"/>
    <field name="duration"/>
    <label name="output"/>
    <field name="output"/>
    <label name="url"/>
    <field name="url"/>
    <label name="planned_works"/>
    <field name="planned_works"/>
    <notebook colspan="4">
        <page name="projects">
            <field name="projects" colspan="4"/>
            <button name="export" string="Export"/>
            <label name="exported_date"/>
            <field name="exported_date"/>
        </page>
        <page name="exported_msg">
            <field name="exported_msg" colspan="4"/>
        </page>
    </notebook>
</form>
